{
  "name": "react-components",
  "homepage": "https://github.com/Abdikhaniyev/rs-school-react/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && prettier --check .",
    "fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix && prettier . --write",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier . --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "next": "14.0.3",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-config-next": "14.0.3",
    "happy-dom": "^12.10.3",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "msw": "^2.0.6",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  }
}
